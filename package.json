{
  "name": "model",
  "version": "0.0.0",
  "main": "./dist/main.js",
  "exports": "./dist/main.js",
  "bin": {
    "orm": "./bin/main.js"
  },
  "scripts": {
    "build": "tsc -b",
    "build:all": "npm run build&&npm run build:cli",
    "build:cli": "tsc -p tsconfig.cli.json",
    "cli":"node bin/main.js",
    "check": "biome check",
    "check:fix": "biome check --write",
    "dev": "TASK=db tsx --watch src/runner.ts",
    "dev:cli": "ENV=dev tsx --tsconfig tsconfig.cli.json --watch cli/main.ts",
    "migrate": "TASK=migrate tsx --watch src/runner.ts",
    "start": "node dist/main.js"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.6",
    "@types/better-sqlite3": "^7.6.13",
