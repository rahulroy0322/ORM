    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "better-sqlite3": "^12.4.1",
    "chalk": "^5.6.2",
    "joi": "^18.0.1",
    "mongodb": "^7.0.0",
    "mysql2": "^3.15.3",
    "pg": "^8.16.3"
  },
